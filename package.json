{
  "name": "pharmacy-data-etl",
  "version": "0.1.0",
  "description": "ETL to extract data from Syndication/Organisations and store as JSON",
  "main": "app.js",
  "scripts": {
    "coverage-check": "istanbul check-coverage --config .istanbul.yml",
    "coverage-generate": "yarn run istanbul -- cover _mocha -- --recursive test",
    "coverage-upload-coveralls": "cat ./coverage/lcov.info | coveralls",
    "git-hook": "yarn run lint && yarn run coverage-generate && yarn run coverage-check",
    "istanbul": "NODE_ENV=test istanbul",
    "lint": "eslint --ext .js,.json .",
    "postrewrite": "yarn run git-hook",
    "precommit": "yarn run git-hook",
    "prepush": "yarn run git-hook",
    "snyk": "snyk",
    "snyk-travis": "yarn run snyk -- test --org=nhsuk && yarn run snyk -- monitor --org=nhsuk",
    "start": "node schedule.js",
    "start-clear": "node schedule.js clear",
    "start-small": "node schedule.js small",
    "start-small-clear": "node schedule.js small clear",
    "test": "NODE_ENV=test mocha --recursive test",
    "test-ci": "yarn run lint && yarn run coverage-generate && yarn run coverage-check && yarn run coverage-upload-coveralls && yarn run snyk-travis",
    "test-integration": "NODE_ENV=test mocha --recursive test/integration",
    "test-unit": "NODE_ENV=test mocha --recursive test/unit",
    "test-watch": "yarn run test -- --watch --reporter min",
    "test-watch-integration": "yarn run test-integration -- --watch --reporter min",
    "test-watch-unit": "yarn run test-unit -- --watch --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsuk/pharmacy-data-etl.git"
  },
  "keywords": [
    "Pharmacy",
    "JSON",
    "Organisation",
    "Syndication"
  ],
  "author": "Mark Harrop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhsuk/pharmacy-data-etl/issues"
  },
  "homepage": "https://github.com/nhsuk/pharmacy-data-etl#readme",
  "devDependencies": {
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-nhsuk": "^0.3.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-watch": "^3.1.2",
    "husky": "^0.14.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "snyk": "^1.36.2"
  },
  "dependencies": {
    "async": "^2.5.0",
    "azure-storage": "^2.2.1",
    "bunyan": "^1.8.10",
    "google-libphonenumber": "^3.0.0",
    "node-schedule": "^1.2.3",
    "require-environment-variables": "^1.1.2",
    "xml2js": "^0.4.17"
  }
}
